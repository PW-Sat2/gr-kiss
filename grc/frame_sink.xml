<?xml version="1.0"?>
<block>
  <name>Frame sink</name>
  <key>frame_sink</key>
  <category>gr_kiss</category>
  <import>import gr_kiss</import>
  <make>gr_kiss.frame_sink($addr)</make>

  <param>
    <name>Address</name>
    <key>addr</key>
    <value></value>
    <type>string</type>
  </param>

  <sink>
    <name>in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
</block>
